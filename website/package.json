    "@testing-library/user-event": "^12.8.3",
    "antd": "^4.17.2",
    "aws-amplify": "^4.3.8",
    "aws-sdk": "^2.1123.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^5.0.0",
    "web-vitals": "^1.1.2"
